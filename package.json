    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "format": "prettier --write src/**/*.jsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
